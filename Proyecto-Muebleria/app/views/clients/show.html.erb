<p id="notice"><%= notice %></p>
<div class="container">
  <h1>Lista de Clientes</h1>
  <div class="row">
    <div class="col-md-8">
      <div class="table-responsive">
        <table class="table table-bordered">
          <thead>
          <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Domicilio</th>
            <th>Telefono</th>
            <th>Email</th>
          </tr>
          <tr>
            <td><%= @client.nombre  %></td>
            <td><%= @client.apellido  %></td>
            <td><%= @client.domicilio %></td>
            <td><%= @client.telefono %></td>
            <td><%= @client.email %></td>

          </tr>
          </thead>

        </table>


<%= link_to 'Editar', edit_client_path(@client), class: "btn btn-info" %> |
<%= link_to 'Regresar', clients_path, class: "btn btn-warning" %>
    </div>
  </div>
</div>
</div>